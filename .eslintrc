{
    "env": {
        "browser": true,
        "es2021": true
    },
    "plugins": ["@typescript-eslint"],
    "extends": [
        "react-app",
        "plugin:prettier/recommended",
        "eslint:recommended"
    ],
    "parser": "@typescript-eslint/parser",
    "rules": {
        "prettier/prettier": [
            "error",
            { "singleQuote": true, "endOfLine": "auto", "tabWidth": 4 }
        ]
    }
}
